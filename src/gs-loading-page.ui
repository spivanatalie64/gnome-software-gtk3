<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="3.0"/><template class="GsLoadingPage" parent="GsPage">
    <child>
      <object class="GtkBox" id="status_page">
        <property name="icon_name">org.gnome.Software</property>
        <property name="title" translatable="yes">Starting Up</property>
        <style>
          <class name="icon-dropshadow"/>
        </style>
        <child>
          <object class="GtkBox">
            <property name="maximum-size">400</property>
            <child>
              <object class="GtkProgressBar" id="progressbar">
                <property name="fraction">0.0</property>
                <property name="margin_bottom">12</property>
                <style>
                  <class name="upgrade-progressbar"/>
                </style>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
