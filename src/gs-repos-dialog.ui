<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="3.20"/>
  <template class="GsReposDialog" parent="GtkDialog">
    <property name="title" translatable="yes">Software Repositories</property>
    <property name="default-width">640</property>
    <property name="default-height">576</property>
    <property name="child">
      <object class="GtkBox">
        <child type="top">
          <object class="GtkHeaderBar">
            <property name="title-widget">
              <object class="GtkLabel">
                <property name="title" bind-source="GsReposDialog" bind-property="title" bind-flags="sync-create"/>
              </object>
            </property>
          </object>
        </child>
        <property name="content">
          <object class="GtkOverlay" id="toast_overlay">
            <property name="child">
              <object class="GtkStack" id="stack">
                <property name="vexpand">True</property>

