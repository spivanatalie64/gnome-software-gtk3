<?xml version='1.0' encoding='UTF-8'?>
<interface>
  <requires lib="gtk+" version="3.0" />  <object class="GtkShortcutsDialog" id="shortcuts_dialog">
    <child>
      <object class="GtkShortcutsSection">
        <property name="title" translatable="yes">Navigation</property>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">&lt;Alt&gt;1</property>
            <property name="title" translatable="yes">Navigate to Explore Page</property>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">&lt;Alt&gt;2</property>
            <property name="title" translatable="yes">Navigate to Installed Page</property>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">&lt;Alt&gt;3</property>
            <property name="title" translatable="yes">Navigate to Updates Page</property>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">&lt;Control&gt;F</property>
            <property name="title" translatable="yes">Open Search Bar</property>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">F10</property>
            <property name="title" translatable="yes">Open Main Menu</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkShortcutsSection">
        <property name="title" translatable="yes">Windows</property>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">&lt;Control&gt;comma</property>
            <property name="title" translatable="yes">Open Preferences</property>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">&lt;Control&gt;period</property>
            <property name="title" translatable="yes">Open Software Repositories</property>
          </object>
        </child>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">&lt;Alt&gt;F4 &lt;Control&gt;Q</property>
            <property name="title" translatable="yes">Close Main Window</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkShortcutsSection">
        <property name="title" translatable="yes">Updates Page</property>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">F5 &lt;Control&gt;r</property>
            <property name="title" translatable="yes">Check for Updates</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkShortcutsSection">
        <property name="title" translatable="yes">Help</property>
        <child>
          <object class="GtkShortcutsShortcut">
            <property name="accelerator">&lt;Control&gt;question</property>
            <property name="title" translatable="yes">Open Keyboard Shortcuts</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
